datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}

generator db {
  provider = "go run github.com/steebchen/prisma-client-go"
}

model MutantCandidate {
  id        Int     @id @default(autoincrement())
  dnaString String  @unique
  isMutant  Boolean
}
